#!/bin/bash
#Set the desired key size
KEY_SIZE=2048
#Set the desired file path for the key pair
KEY_PATH="$HOME/.ssh/id_rsa"
#Generate the key pair
ssh-keygen -t rsa -b "$KEY_SIZE" -f "$KEY_PATH"-N""
#Print a message indicating the key generation is complete
echo "RSA key pairgenerated successfully."
